\\u0439\\u043C\\u0432\\u043E\\u0440\\u043A - \\u044D\\u0442\\u043E \\u043E\\u043F\\u0440\\u0435\\u0434\\u0435\\u043B\\u0435\\u043D\\u043D\\u0430\\u044F \\u0441\\u0438\\u0441\\u0442\\u0435\\u043C\\u0430, \\u043D\\u0430\\u043F\\u0438\\u0441\\u0430\\u043D\\u043D\\u0430\\u044F \\u0434\\u0440\\u0443\\u0433\\u0438\\u043C\\u0438 \\u0440\\u0430\\u0437\\u0440\\u0430\\u0431\\u043E\\u0442\\u0447\\u0438\\u043A\\u0430\\u043C\\u0438 \\u0447\\u0442\\u043E\\u0431\\u044B \\u0431\\u044B\\u0441\\u0442\\u0440\\u0435\\u0435 \\u0438 \\u043F\\u0440\\u043E\\u0449\\u0435 \\u0440\\u0430\\u0437\\u0440\\u0430\\u0431\\u0430\\u0442\\u044B\\u0432\\u0430\\u0442\\u044C web \\u043F\\u0440\\u0438\\u043B\\u043E\\u0436\\u0435\\u043D\\u0438\\u044F. \\u041C\\u043D\\u043E\\u0433\\u0438\\u0435 \\u0444\\u0440\\u0435\\u0439\\u043C\\u0432\\u043E\\u0440\\u043A\\u0438 \\u0438\\u043C\\u0435\\u044E\\u0442 \\u0432 \\u0441\\u0435\\u0431\\u0435 \\u0443\\u0436\\u0435 \\u0433\\u043E\\u0442\\u043E\\u0432\\u044B\\u0435 \\u0438\\u043D\\u0441\\u0442\\u0440\\u0443\\u043C\\u0435\\u043D\\u0442\\u044B, \\u043A\\u043E\\u0442\\u043E\\u0440\\u044B\\u043C\\u0438 \\u043C\\u043E\\u0436\\u043D\\u043E \\u043F\\u043E\\u043B\\u044C\\u0437\\u0432\\u0430\\u0442\\u044C\\u0441\\u044F \\u043F\\u0440\\u0438 \\u0440\\u0430\\u0437\\u0440\\u0430\\u0431\\u043E\\u0442\\u043A\\u0435 \\u043F\\u0440\\u0438\\u043B\\u043E\\u0436\\u0435\\u043D\\u0438\\u0439, \\u043D\\u0430\\u043F\\u0440\\u0438\\u043C\\u0435\\u0440 \\u0440\\u0430\\u0431\\u043E\\u0442\\u0430 \\u0441 \\u0444\\u043E\\u0440\\u043C\\u0430\\u043C\\u0438 \\u0432 HTML, \\u0438\\u043B\\u0438 \\u0441\\u0431\\u043E\\u043A\\u0430 \\u043F\\u0440\\u043E\\u0435\\u043A\\u0442\\u0430 \\u0432 \\u043E\\u0431\\u0449\\u0438\\u0435 \\u0431\\u0430\\u043D\\u0434\\u043B\\u044B, \\u043A\\u043E\\u0442\\u043E\\u0440\\u044B\\u0435 \\u043F\\u043E\\u0439\\u0434\\u0443\\u0442 \\u0432 production.\"}),`\n`,(0,n.jsx)(e.p,{children:\"\\u041D\\u0430 \\u0440\\u044B\\u043D\\u043A\\u0435 \\u0441\\u0435\\u0439\\u0447\\u0430\\u0441 \\u0435\\u0441\\u0442\\u044C 4 \\u0441\\u0430\\u043C\\u044B\\u0445 \\u043F\\u043E\\u043F\\u0443\\u043B\\u044F\\u0440\\u043D\\u044B\\u0445 \\u0444\\u0440\\u0435\\u0439\\u043C\\u0432\\u043E\\u0440\\u043A\\u0430 \\u0432\\u043E \\u0444\\u0440\\u043E\\u043D\\u0442\\u0435\\u043D\\u0434\\u0435. \\u042D\\u0442\\u043E React, Angular, Vue,Svelte. \\u0427\\u0435\\u0441\\u0442\\u043D\\u043E \\u0433\\u043E\\u0432\\u043E\\u0440\\u044F \\u0438\\u0437 \\u0441\\u0432\\u043E\\u0435\\u0439 \\u043F\\u0440\\u0430\\u043A\\u0442\\u0438\\u043A\\u0438 \\u043A\\u0430\\u043A \\u0440\\u0430\\u0437\\u0440\\u0430\\u0431\\u043E\\u0442\\u0447\\u0438\\u043A\\u0430 \\u044F \\u043F\\u043E\\u043B\\u044C\\u0437\\u043E\\u0432\\u0430\\u043B\\u0441\\u044F \\u0442\\u043E\\u043B\\u044C\\u043A\\u043E \\u0434\\u0432\\u0443\\u043C\\u044F \\u0438\\u0437 \\u043D\\u0438\\u0445 - \\u044D\\u0442\\u043E React \\u0438 Angular, \\u043F\\u043E\\u044D\\u0442\\u043E\\u043C\\u0443 \\u043F\\u0440\\u043E Vue \\u0438 Svelte \\u043D\\u0438\\u0447\\u0435\\u0433\\u043E \\u0441\\u043A\\u0430\\u0437\\u0430\\u0442\\u044C \\u043E\\u0441\\u043E\\u0431\\u043E \\u043D\\u0435 \\u043C\\u043E\\u0433\\u0443. \\u0415\\u0434\\u0438\\u043D\\u0441\\u0442\\u0432\\u0435\\u043D\\u043D\\u043E\\u0435 \\u0441\\u043A\\u0430\\u0436\\u0443 \\u0447\\u0442\\u043E \\u0432\\u0430\\u043A\\u0430\\u043D\\u0441\\u0438\\u0439 \\u043D\\u0430 Svelte \\u0433\\u0434\\u0435 \\u0442\\u043E 1% \\u043D\\u0430 \\u0440\\u044B\\u043D\\u043A\\u0435 \\u0432 \\u0420\\u043E\\u0441\\u0441\\u0438\\u0438 \\u0441\\u0435\\u0439\\u0447\\u0430\\u0441, \\u0434\\u0443\\u043C\\u0430\\u044E \\u0447\\u0442\\u043E \\u0442\\u0430\\u043A\\u0430\\u044F \\u0436\\u0435 \\u043A\\u0430\\u0440\\u0442\\u0438\\u043D\\u0430 \\u0438 \\u0432 \\u0434\\u0440\\u0443\\u0433\\u0438\\u0445 \\u0441\\u0442\\u0440\\u0430\\u043D\\u0430\\u0445 \\u0421\\u041D\\u0413. React - \\u0441\\u0430\\u043C\\u044B\\u0439 \\u043F\\u043E\\u043F\\u0443\\u043B\\u044F\\u0440\\u043D\\u044B\\u0439 \\u0441\\u0440\\u0435\\u043