u0438\\u043D\\u0442\\u0435\\u0440\\u0432\\u044C\\u044E \\u0431\\u043B\\u0430 \\u0431\\u043B\\u0430 \\u0431\\u043B\\u0430...\". \\u041D\\u0443 \\u0442\\u0443\\u0442 \\u044F \\u043F\\u043E\\u0434\\u0443\\u043C\\u0430\\u043B\\r\n\\u0434\\u0430\\u0439-\\u043A\\u0430 \\u043F\\u043E\\u043F\\u0440\\u0443\\u0431\\u0443\\u044E \\u0432 \\u044D\\u0442\\u043E\\u0442 \\u0440\\u0430\\u0437, \\u043D\\u0435 \\u0431\\u0443\\u0434\\u0443 \\u043E\\u0442\\u043A\\u0430\\u0437\\u044B\\u0432\\u0430\\u0442\\u044C \\u0438 \\u043F\\u0440\\u043E\\u0432\\u0435\\u0440\\u044E \\u0441\\u0432\\u043E\\u0438 \\u0441\\u0438\\u043B\\u044B =). \\u041E\\u0431\\u0430 \\u0441\\u043E\\u0431\\u0435\\u0441\\u0430 \\u0431\\u044B\\u043B\\u0438 \\u0432 \\u043E\\u0434\\u043D\\u0438 \\u0438\\u0437 \\u0441\\u0430\\u043C\\u044B\\u0445\\r\n\\u043A\\u0440\\u0443\\u043F\\u043D\\u044B\\u0445 \\u043A\\u043E\\u043C\\u043F\\u0430\\u043D\\u0438\\u0439 \\u0432 \\u0420\\u0424 \\u0438 \\u0441\\u0443\\u043C\\u043C\\u0430 \\u043F\\u0440\\u0435\\u0434\\u043B\\u043E\\u0436\\u0435\\u043D\\u0438\\u044F \\u0431\\u044B\\u043B\\u0430 \\u0432 \\u0440\\u0430\\u0439\\u043E\\u043D\\u0435 280\\u043A \\u0441 \\u043E\\u0431\\u0435\\u0438\\u0445 \\u0441\\u0442\\u043E\\u0440\\u043E\\u043D. \\u0412 \\u0434\\u0430\\u043D\\u043D\\u043E\\u043C \\u0441\\u043B\\u0443\\u0447\\u0430\\u0435 \\u044D\\u0442\\u043E \\u0443\\u0440\\u043E\\u0432\\u0435\\u043D\\u044C middle/senior\\r\nfrontend \\u0440\\u0430\\u0437\\u0440\\u0430\\u0431\\u043E\\u0442\\u0447\\u0438\\u043A\\u0430. \\u041D\\u0430\\u0437\\u0432\\u0430\\u043D\\u0438\\u044F \\u043A\\u043E\\u043C\\u043F\\u0430\\u043D\\u0438\\u0439 \\u044F \\u043D\\u0435 \\u0431\\u0443\\u0434\\u0443 \\u0440\\u0430\\u0441\\u043A\\u0440\\u044B\\u0432\\u0430\\u0442\\u044C, \\u043D\\u043E \\u043D\\u0430\\u0437\\u043E\\u0432\\u0435\\u043C \\u0438\\u0445 \\u043A\\u043E\\u043C\\u043F\\u0430\\u043D\\u0438\\u044F \\u0410 - \\u0433\\u0434\\u0435 \\u0431\\u044B\\u043B \\u043D\\u0443 \\u043F\\u0440\\u044F\\u043C \\u0441\\u0443\\